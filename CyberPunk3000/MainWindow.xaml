<Window x:Class="WpfCyberPunk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCyberPunk"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="BtnCreate" Content="Créer un personnage" HorizontalAlignment="Left" Margin="15,10,0,0" VerticalAlignment="Top" Width="200"/>
        <Button Name="BtnImport" Content="Importer un personnage" Click="BtnImport_Click" HorizontalAlignment="Left" Margin="15,50,0,0" VerticalAlignment="Top" Width="200"/>
        <Button Name="BtnOpen" Content="Ouvrir feuilles de personnage" HorizontalAlignment="Left" Margin="15,90,0,0" VerticalAlignment="Top" Width="200"/>
    </Grid>
</Window>
